{
  "name": "test-kms",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "server": "npm run decrypt:credentials && npm run start",
    "build:client:prod:ds": "npm run decrypt:credentials:prod:ds && npm run start",
    "encrypt:credentials:prod:ds": "node -e 'require(\"./public/javascripts/encrypt.js\").encrypt()' NODE_ENV=ds MODE_ENV=prod",
    "decrypt:credentials:prod:ds": "node -e 'require(\"./public/javascripts/decrypt.js\").decrypt()' NODE_ENV=ds MODE_ENV=prod",

    "build:client:local:ds": "npm run decrypt:credentials:prod:ds && npm run start",
    "encrypt:credentials:local:ds": "node -e 'require(\"./public/javascripts/encrypt.js\").encrypt()' NODE_ENV=ds MODE_ENV=local",
    "decrypt:credentials:local:ds": "node -e 'require(\"./public/javascripts/decrypt.js\").decrypt()' NODE_ENV=ds MODE_ENV=local",

    "upload:credentials": "node -e 'require(\"./public/javascripts/upload-file.js\").uploadFile()'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run encrypt:credentials:prod:ds && npm run encrypt:credentials:local:ds && git add credentials-enc/*.txt.encrypted"
    }
  },
  "dependencies": {
    "@google-cloud/kms": "1.1.1",
    "@google-cloud/storage": "2.5.0",
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "dotenv": "4.0.0",
    "envkey": "1.2.6",
    "express": "4.16.1",
    "googleapis": "40.0.0",
    "morgan": "1.9.1",
    "node-sass-middleware": "0.11.0",
    "nodemon": "1.19.1",
    "request-promise": "4.2.4",
    "util": "0.12.0"
  },
  "devDependencies": {
    "husky": "2.6.0"
  }
}
